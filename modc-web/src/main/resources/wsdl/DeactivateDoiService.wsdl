            <element name="DeactivateDoiRequest">
                <annotation>
                    <documentation>DeactivateDoiRequest for deactivating a doi.</documentation>
                </annotation>
                <complexType>
                    <sequence>
                        <element name="serviceId" type="string"/>
                        <element name="doi" type="doi:Doi"/>
                    </sequence>
                </complexType>
            </element>
            <element name="DeactivateDoiResponse">
                <annotation>
                    <documentation>DeactivateDoiResponse for deactivating a doi.</documentation>
                </annotation>
                <complexType>
                    <sequence>
                        <element name="serviceId" type="string"/>
                        <element name="responsecode" type="doi:DResponseCode"/>
                        <element name="message" type="string"/>
                        <element name="doi" type="doi:Doi"/>
                        <element name="url" type="string"/>
                        <element name="verbosemessage" type="string"/>
                    </sequence>
                    <attribute name="type" type="doi:DResponseTypeAtt" use="required"/>
                </complexType>
            </element>
            <simpleType name="DResponseCode">
                <restriction base="string">
                    <enumeration value="MT001"/>
                    <enumeration value="MT002"/>
                    <enumeration value="MT003"/>
                    <enumeration value="MT004"/>
                    <enumeration value="MT005"/>
                    <enumeration value="MT006"/>
                    <enumeration value="MT007"/>
                    <enumeration value="MT008"/>
                    <enumeration value="MT009"/>
                    <enumeration value="MT010"/>
                    <enumeration value="MT011"/>
                    <enumeration value="MT012"/>
                    <enumeration value="MT013"/>
                    <enumeration value="MT090"/>
                    <enumeration value="MT091"/>
                </restriction>
            </simpleType>
            <simpleType name="DResponseTypeAtt">
                <restriction base="string">
                    <enumeration value="success"/>
                    <enumeration value="failure"/>
                </restriction>
            </simpleType>
            <simpleType name="Doi">
                <restriction base="token">
                    <pattern value="[1][0][/.].*"/>
                </restriction>
            </simpleType>
        </schema>
    </wsdl:types>
    <wsdl:message name="DeactivateDoiRequest">
        <wsdl:part element="tns:DeactivateDoiRequest" name="DeactivateDoiRequest">
        </wsdl:part>
    </wsdl:message>
    <wsdl:message name="DeactivateDoiResponse">
        <wsdl:part element="tns:DeactivateDoiResponse" name="DeactivateDoiResponse">
        </wsdl:part>
    </wsdl:message>
    <wsdl:portType name="DeactivateDoiService">
        <wsdl:operation name="DeactivateDoi">
            <wsdl:input message="tns:DeactivateDoiRequest" name="DeactivateDoiRequest">
            </wsdl:input>
            <wsdl:output message="tns:DeactivateDoiResponse" name="DeactivateDoiResponse">
            </wsdl:output>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="DeactivateDoiServiceSoap11" type="tns:DeactivateDoiService">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="DeactivateDoi">
            <soap:operation soapAction=""/>
            <wsdl:input name="DeactivateDoiRequest">
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output name="DeactivateDoiResponse">
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:service name="DeactivateDoiService">
