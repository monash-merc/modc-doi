<jaxb:bindings
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
        jaxb:version="2.0">
    <jaxb:globalBindings>
        <xjc:javaType name="java.util.Date" xmlType="xsd:dateTime" adapter="edu.monash.merc.eddy.modc.ws.jaxbadapter.JaxbDateTimeAdapter"/>
        <xjc:javaType name="java.util.Date" xmlType="xsd:date" adapter="edu.monash.merc.eddy.modc.ws.jaxbadapter.JaxbDateAdapter"/>
    </jaxb:globalBindings>
</jaxb:bindings>